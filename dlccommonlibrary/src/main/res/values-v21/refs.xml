<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--默认点击背景-->
    <item name="clickable_bg" type="drawable">@drawable/ripple_default</item>
    <!--无边界主题色的点击背景-->
    <item name="clickable_bg_borderless" type="drawable">@drawable/ripple_default_borderless</item>
    <!--方形按钮点击背景-->
    <item name="clickable_bg_item" type="drawable">@drawable/ripple_default_action_item</item>
    <!--未点击时，颜色为白色，点击后涟漪-->
    <item name="clickable_bg_white" type="drawable">@drawable/ripple_default_white_selector</item>
    <!--低版本为透明，高版本为涟漪效果-->
    <item name="clickable_trans" type="drawable">@drawable/ripple_default</item>

</resources>